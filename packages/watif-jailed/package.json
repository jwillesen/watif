{
  "name": "watif-jailed",
  "version": "0.0.0",
  "description": "A utility for running watif stories in a jailed environment",
  "main": "lib/index.js",
  "scripts": {
    "build": "npm run build:bootstrap && npm run build:lib",
    "build:bootstrap": "webpack",
    "build:bootstrap:watch": "npm run build:bootstrap -- --watch",
    "build:lib": "babel src -d lib --ignore __tests__",
    "build:lib:watch": "npm run build:lib -- --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "clean": "rm -rf lib dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jwillesen/watif.git"
  },
  "keywords": [
    "interactive",
    "fiction"
  ],
  "author": "Jon Willesen",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jwillesen/watif/issues"
  },
  "homepage": "https://github.com/jwillesen/watif#readme",
  "dependencies": {
    "jailed": "^0.3.1",
    "utils-error-to-json": "^1.0.0",
    "watif-interpreter": "^0.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "jest": "^22.4.4",
    "webpack": "^4.8.3"
  }
}
